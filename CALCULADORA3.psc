Función sumanumeros(n1,n2)
	Definir suma Como Entero
	suma <- n1+n2
	Escribir 'la suma de los numeros es : ', suma
FinFunción

Función multiplicacion(n1,n2)
	Definir mult Como Entero
	mult <- n1*n2
	Escribir 'la multiplicacion de los numeros es :', mult
FinFunción

Algoritmo CALCULADORA
	sumanumeros(2,3)
	sumanumeros(54,37)
	multiplicacion(12,5)
FinAlgoritmo
