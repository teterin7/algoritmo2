funcion restanumeros(n1,n2)
	
	resta= n1-n2
	mostrar "la resta de los numeros es : " ,resta
	
FinFuncion

funcion dividirnumeros(n1,n2)
	dividir= n1/n2
	mostrar" la division de los numeros es :" , dividir

FinFuncion

